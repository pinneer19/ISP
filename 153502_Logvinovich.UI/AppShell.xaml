<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="_153502_Logvinovich.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:_153502_Logvinovich.UI"
    xmlns:pages="clr-namespace:_153502_Logvinovich.UI.Pages">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Superheroes"
        ContentTemplate="{DataTemplate pages:Superheroes}"
        Route="Superheroes" />

</Shell>
